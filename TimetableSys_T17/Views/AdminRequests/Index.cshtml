@model IEnumerable<TimetableSys_T17.Request>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            RequestID
        </th>
        <th>
            Start Date
        </th>
        <th>
            End Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.requestID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.moduleID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sessionTypeID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dayID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.moduleID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sessionTypeID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.round }) |
            @Html.ActionLink("Details", "Details", new { id=item.round }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.round })
        </td>
    </tr>
}

</table>
